<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Extension Name</key>
	<string>Github Mate</string>
	<key>Extension Description</key>
	<string>download Github code snippet </string>
	<key>Extension Identifier</key>
	<string>popclip.extension.GithubMate</string>
	<key>Version</key>
	<integer>1</integer>
	<key>Actions</key>
	<array>
		<dict>
			<key>Title</key>
                        <string>Github Mate</string>
                        <key>Regular Expression</key>
                        <string>.*github.com/.*master.*</string>
			<key>Script Interpreter</key>
			<string>/usr/bin/python</string>
			<key>Shell Script File</key>
			<string>retrieve.py</string>
			<key>Image File</key>
			<string>github.png</string>
		</dict>
	</array>
</dict>
</plist>
